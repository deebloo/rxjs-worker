{
  "name": "redux-web-worker",
  "version": "1.0.1",
  "description": "A redux implementation with web workers",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run es2015 && npm run global-min && npm run global-dev",
    "global-min": "NODE_ENV=prod webpack",
    "global-dev": "NODE_ENV=dev webpack",
    "es2015": "tsc --outDir build/ --target ES2015 --module ES2015 --declaration",
    "release": "npm run build && npm publish",
    "test": "karma start",
    "postinstall": "typings install"
  },
  "keywords": [
    "web workers",
    "rxjs",
    "observables"
  ],
  "files": [
    "build/src",
    "build/bundles"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deebloo/redux-workers.git"
  },
  "author": "deebloo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deebloo/redux-workers/issues"
  },
  "homepage": "https://github.com/deebloo/redux-workers",
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-remap-istanbul": "^0.1.0",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "remap-istanbul": "^0.6.4",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "rxjs": "^5.0.0-beta.6"
  }
}
